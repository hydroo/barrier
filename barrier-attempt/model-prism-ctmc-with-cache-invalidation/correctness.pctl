P>=1  [F ("shared_2")]
P>=1  [F ("invalid_2")]
P>=1  [F ("modified_3")]
P>=1  [F ("shared_3")]
P>=1  [F ("invalid_3")]

((S=? [ "modified_1" ] - S=? [ "modified_2" ]) <= error) | (error*-1 <= (S=? [ "modified_1" ] - S=? [ "modified_2" ]))
((S=? [ "modified_2" ] - S=? [ "modified_3" ]) <= error) | (error*-1 <= (S=? [ "modified_1" ] - S=? [ "modified_2" ]))

((S=? [ "shared_1" ] - S=? [ "shared_2" ]) <= error) | (error*-1 <= (S=? [ "shared_1" ] - S=? [ "shared_2" ]))
((S=? [ "shared_2" ] - S=? [ "shared_3" ]) <= error) | (error*-1 <= (S=? [ "shared_1" ] - S=? [ "shared_2" ]))

// implicitly correct if the 4 above are
//((S=? [ "invalid_1" ] - S=? [ "invalid_2" ]) <= error) | (error*-1 <= (S=? [ "invalid_1" ] - S=? [ "invalid_2" ]))
//((S=? [ "invalid_2" ] - S=? [ "invalid_3" ]) <= error) | (error*-1 <= (S=? [ "invalid_1" ] - S=? [ "invalid_2" ]))

