const double error=1.0E-6;

// cache properties
P<=0 [G ("modified_1" & "modified_2")]
P<=0 [G ("modified_1" & "modified_3")]
P<=0 [G ("modified_2" & "modified_1")]
P<=0 [G ("modified_2" & "modified_3")]
P<=0 [G ("modified_3" & "modified_1")]
P<=0 [G ("modified_3" & "modified_2")]

P<=0 [G ("shared_1" & !("shared_2" | "shared_3"))]
P<=0 [G ("shared_2" & !("shared_1" | "shared_3"))]
P<=0 [G ("shared_3" & !("shared_1" | "shared_2"))]

P>=1  [F ("modified_1")]
